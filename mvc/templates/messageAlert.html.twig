{% extends "_layout.html.twig" %}

{#--------------------------------#}

{% block title %}Customer Care Map{% endblock %}

{#--------------------------------#}

{% block style %}
    <style>
        @import "css/bootstrap.min.css";
        @import "css/custom.css";
    </style>
{% endblock %}



{#--------------------------------#}
{% block main %}
<main>
    <div class="row">
        <div class="col-sm-12">
            <ol class="breadcrumb">
               <li class="active">{{ forwarding }}</li>
            </ol>
            <div {{ messageAlertClass | raw }} id="messageAlert">
